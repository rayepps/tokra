{
  "name": "tokra",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "publish-packages": "turbo run build lint test && changeset version && changeset publish"
  },
  "devDependencies": {
    "@types/node": "^18.7.23",
    "changeset": "^0.2.6",
    "prettier": "latest",
    "ts-node": "^10.9.1",
    "tslint": "^6.0.0",
    "tsup": "^6.2.3",
    "turbo": "latest",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "radash": "^7.1.0"
  },
  "packageManager": "yarn@1.22.19"
}
