{
  "name": "tokra-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "lint": "lerna run lint",
    "format": "prettier --write \"**/*.{ts,md}\""
  },
  "commands": {
    "publish": {
      "ignoreChanges": [
        "**/*.md",
        "**/example/**",
        "**/*.test.ts"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^18.7.23",
    "lerna": "^4.0.0",
    "prettier": "latest",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.19"
}
